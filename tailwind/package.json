{
  "name": "dumb-package",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "tailwind -i index.css -o ../config-ui/src/style/tailwind.css",
    "watch": "tailwind --watch -i index.css -o ../config-ui/src/style/tailwind.css",
    "build-help-page-css": "tailwind -c help_page.config.js -i help_page.css -o ../bin/site/css/HelpPage.css",
    "watch-help-page-css": "tailwind -c help_page.config.js -i help_page.css -o ../config-ui/src/style/HelpPage.css --watch",
    "build-html-tools-css": "tailwind -c html-tools.config.js -i help_page.css -o ../bin/site/css/tailwind.css",
    "watch-html-tools-css": "tailwind -c html-tools.config.js -i help_page.css -o ../bin/html-tools/tailwind.css --watch"
  },
  "devDependencies": {
    "tailwindcss": "^3.0.23"
  }
}
